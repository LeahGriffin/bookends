<div class="booyah-box col-10 offset-1">  
  <%= simple_form_for @medium do |f| %>
    <%= f.input :genre %>
    <%= f.input :publisher %>
    <%= f.input :title %>
    <%= f.input :author %>
    <%= f.input :description %>
    <%= f.submit "Fixit!!", class: 'btn btn-primary' %>
  <% end %>
</div>  